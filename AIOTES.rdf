<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Alignment [
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY exmo "http://exmo.inrialpes.fr/align/ext/1.0/#">
  <!ENTITY unit "http://ontology.universAAL.org/Unit.owl#">
  <!ENTITY context "http://ontology.universAAL.org/Context.owl#">
  <!ENTITY device "http://ontology.universAAL.org/Device.owl#">
  <!ENTITY uaal "http://ontology.universAAL.org/">
  <!ENTITY healthMeasurement "http://ontology.universaal.org/HealthMeasurement.owl#">
  <!ENTITY measurement "http://ontology.universaal.org/Measurement.owl#">
  <!ENTITY sripas "http://www.inter-iot.eu/sripas#">
  <!ENTITY sosa "http://www.w3.org/ns/sosa/">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
  <!ENTITY ssn "http://www.w3.org/ns/ssn/">
  <!ENTITY iiotex "http://inter-iot.eu/GOIoTPex#">
  <!ENTITY medex "http://inter-iot.eu/medex#">
  <!ENTITY iiot "http://inter-iot.eu/GOIoTP#">
  <!ENTITY time "http://www.w3.org/2006/time#">
  <!ENTITY s2 "http://inter-iot.eu/syntax/SOFIA2#">
  <!ENTITY s2mod "http://inter-iot.eu/SOFIA2mod#">
  <!ENTITY sweet_units "http://sweet.jpl.nasa.gov/2.3/reprSciUnits.owl#">
  <!ENTITY aha "http://ontologies.activageproject.eu/aha-ontology#">
  <!ENTITY m3 "http://purl.org/iot/vocab/m3-lite#">
]>
<rdf:RDF 
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
	xmlns="http://www.inter-iot.eu/sripas#" 
	xmlns:align="http://knowledgeweb.semanticweb.org/heterogeneity/alignment#" 
	xmlns:co="http://inter-iot.eu/central" 
	xmlns:context="http://ontology.universAAL.org/Context.owl#" 
	xmlns:dcelem="http://purl.org/dc/elements/1.1/" 
	xmlns:device="http://ontology.universAAL.org/Device.owl#" 
	xmlns:exmo="http://exmo.inrialpes.fr/align/ext/1.0/#" 
	xmlns:geo-sf="http://www.opengis.net/def/sf/" 
	xmlns:geosparql="http://www.opengis.net/ont/geosparql#" 
	xmlns:healthMeasurement="http://ontology.universaal.org/HealthMeasurement.owl#" 
	xmlns:iiot="http://inter-iot.eu/GOIoTP#" 
	xmlns:m3="http://purl.org/iot/vocab/m3-lite" 
	xmlns:measurement="http://ontology.universaal.org/Measurement.owl#" 
	xmlns:saref="https://w3id.org/saref#" 
	xmlns:saref-ps="http://ontology.tno.nl/saref/positionsensor#" 
	xmlns:sosa="http://www.w3.org/ns/sosa/" 
	xmlns:sripas="http://www.inter-iot.eu/sripas#" 
	xmlns:sweet="http://sweetontology.net/reprSciUnits/" 
	xmlns:time="http://www.w3.org/2006/time#" 
	xmlns:uaal="http://ontology.universAAL.org/" 
	xmlns:unit="http://ontology.universAAL.org/Unit.owl#" 
	xmlns:wgs84_pos="http://www.w3.org/2003/01/geo/wgs84_pos#" 
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	<align:Alignment>
		<dcelem:title>AIOTES-universAAL'Alignment</dcelem:title>
		<exmo:version>1.5</exmo:version>
		<dcelem:creator>amedrano</dcelem:creator>
		<dcelem:description>Alignment between AIOTES Datamodel and universAAL Core Ontologies</dcelem:description>
		<dcelem:date>Wed Jun 19 11:29:56 GMT 2019</dcelem:date>
		<align:onto1>
			<align:Ontology rdf:about="http://www.inter-iot.eu/central">
				<align:formalism>
					<align:Formalism align:name="OWL2.0" align:uri="http://www.w3.org/2002/07/owl#" />
				</align:formalism>
			</align:Ontology>
		</align:onto1>
		<align:onto2>
			<align:Ontology rdf:about="http://ontology.universaal.org/PhThing.owl#">
				<align:formalism>
					<align:Formalism align:name="OWL2.0" align:uri="http://www.w3.org/2002/07/owl#" />
				</align:formalism>
			</align:Ontology>
		</align:onto2>
		<sripas:steps rdf:parseType="Literal">
			<sripas:step sripas:order="1" sripas:cell="http://www.inter-iot.eu/sripas#PresenceSensor" />
			<sripas:step sripas:order="2" sripas:cell="http://www.inter-iot.eu/sripas#COSensor" />
			<sripas:step sripas:order="3" sripas:cell="http://www.inter-iot.eu/sripas#SmokeSensor" />
			<sripas:step sripas:order="4" sripas:cell="http://www.inter-iot.eu/sripas#FallSensor" />
			<sripas:step sripas:order="5" sripas:cell="http://www.inter-iot.eu/sripas#ContactSensor" />
			<sripas:step sripas:order="6" sripas:cell="http://www.inter-iot.eu/sripas#TemperatureSensor" />
			<sripas:step sripas:order="7" sripas:cell="http://www.inter-iot.eu/sripas#HumiditySensor" />
			<sripas:step sripas:order="8" sripas:cell="http://www.inter-iot.eu/sripas#LightSensor" />
			<sripas:step sripas:order="9" sripas:cell="http://www.inter-iot.eu/sripas#AdaptContextProvider" />
			<sripas:step sripas:order="10" sripas:cell="http://www.inter-iot.eu/sripas#CreateCEFromDeviceObservation" />
			<sripas:step sripas:order="11" sripas:cell="http://www.inter-iot.eu/sripas#CreateCEFromSimpleDeviceObservation" />
			<sripas:step sripas:order="12" sripas:cell="http://www.inter-iot.eu/sripas#timeContextEvent" />
			<sripas:step sripas:order="13" sripas:cell="http://www.inter-iot.eu/sripas#timeContextEventFromResultTime" />
			<sripas:step sripas:order="14" sripas:cell="http://www.inter-iot.eu/sripas#bloodPreassureMeasurement" />
			<sripas:step sripas:order="15" sripas:cell="http://www.inter-iot.eu/sripas#changeUser" />
			<sripas:step sripas:order="16" sripas:cell="http://www.inter-iot.eu/sripas#changeUserProp" />
		</sripas:steps>
		<sripas:cellFormat>
			<iiot:DataFormat rdf:about="http://inter-iot.eu/sripas#turtle" />
		</sripas:cellFormat>
		<align:map>
			<align:Cell rdf:about="http://www.inter-iot.eu/sripas#PresenceSensor">
				<align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a  m3:PresenceDetector , sripas:node_CTY] .</align:entity1>
				<align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a  device:PresenceSensor , sripas:node_CTY ] .</align:entity2>
				<align:relation>=</align:relation>
			</align:Cell>
		</align:map>
		<align:map>
			<align:Cell rdf:about="http://www.inter-iot.eu/sripas#COSensor">
				<align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a  m3:GaseousPollutantSensor , sripas:node_CTY] .</align:entity1>
				<align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a  device:COSensor , sripas:node_CTY ] .</align:entity2>
				<align:relation>=</align:relation>
			</align:Cell>
		</align:map>
		<align:map>
			<align:Cell rdf:about="http://www.inter-iot.eu/sripas#SmokeSensor">
				<align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a  &lt;http://casas.wsu.edu/owl/cose.owl#SmokeAlarm&gt; , sripas:node_CTY] .</align:entity1>
				<align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a  device:SmokeSensor , sripas:node_CTY ] .</align:entity2>
				<align:relation>=</align:relation>
			</align:Cell>
		</align:map>
		<align:map>
			<align:Cell rdf:about="http://www.inter-iot.eu/sripas#FallSensor">
				<align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a  m3:FallDetector , sripas:node_CTY] .</align:entity1>
				<align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a  device:FallSensor , sripas:node_CTY ] .</align:entity2>
				<align:relation>=</align:relation>
			</align:Cell>
		</align:map>
		<align:map>
			<align:Cell rdf:about="http://www.inter-iot.eu/sripas#ContactSensor">
				<align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a  m3:DoorStateSensor , sripas:node_CTY] .</align:entity1>
				<align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a  device:ContactSensor , sripas:node_CTY ] .</align:entity2>
				<align:relation>=</align:relation>
			</align:Cell>
		</align:map>
		<align:map>
			<align:Cell rdf:about="http://www.inter-iot.eu/sripas#HumiditySensor">
				<align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a  &lt;http://purl.oclc.org/NET/ssnx/meteo/aws#HumiditySensor&gt; , sripas:node_CTY] .</align:entity1>
				<align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a  device:HumiditySensor , sripas:node_CTY ] .</align:entity2>
				<align:relation>=</align:relation>
			</align:Cell>
		</align:map>
		<align:map>
			<align:Cell rdf:about="http://www.inter-iot.eu/sripas#TemperatureSensor">
				<align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a  m3:AirThermometer , sripas:node_CTY] .</align:entity1>
				<align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a  device:TemperatureSensor , sripas:node_CTY ] .</align:entity2>
				<align:relation>=</align:relation>
			</align:Cell>
		</align:map>
		<align:map>
			<align:Cell rdf:about="http://www.inter-iot.eu/sripas#LightSensor">
				<align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a  m3:LightSensor , sripas:node_CTY] .</align:entity1>
				<align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a  device:LightSensor , sripas:node_CTY ] .</align:entity2>
				<align:relation>=</align:relation>
			</align:Cell>
		</align:map>
		<align:map>
			<align:Cell rdf:about="http://www.inter-iot.eu/sripas#AdaptContextProvider">
				<!--TODO check if more general alignment applies -->
				<align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a       &lt;http://inter-iot.eu/medex#Gauge&gt; , sosa:Platform , sripas:node_CTZ ] .</align:entity1>
				<align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a       context:ContextProvider , sripas:node_CTZ ] .</align:entity2>
				<align:relation>=</align:relation>
			</align:Cell>
		</align:map>
		<align:map>
			<align:Cell rdf:about="http://www.inter-iot.eu/sripas#CreateCEFromDeviceObservation">
				<align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a                    sosa:Observation, sripas:node_CTX ;
            sosa:hasResult       [ a  sripas:node_CTZ ] ;
            sosa:madeBySensor    [ a                sripas:node_CTY ;
                                   sosa:isHostedBy  [ a  sripas:node_CTP ]
                                 ] ;
            
                                 ]
          ] .</align:entity1>
				<align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a                     context:ContextEvent, sripas:node_CTX ;
            rdf:object            [ a  sripas:node_CTZ ] ;
            rdf:predicate         device:hasValue ;
            rdf:subject           [ a  sripas:node_CTY ] ;
            context:hasProvider   [ a  sripas:node_CTP ] 
          ] .</align:entity2>
				<align:relation>=</align:relation>
			</align:Cell>
		</align:map>
		<align:map>
			<align:Cell rdf:about="http://www.inter-iot.eu/sripas#CreateCEFromSimpleDeviceObservation">
				<align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a                    sosa:Observation, sripas:node_CTX ;
            sosa:hasSimpleResult       [ a  sripas:node_CTZ ] ;
            sosa:madeBySensor    [ a                sripas:node_CTY ;
                                   sosa:isHostedBy  [ a  sripas:node_CTP ]
                                 ] ;
            
                                 ]
          ] .</align:entity1>
				<align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a                     context:ContextEvent, sripas:node_CTX ;
            rdf:object            [ a  sripas:node_CTZ ] ;
            rdf:predicate         device:hasValue ;
            rdf:subject           [ a  sripas:node_CTY ] ;
            context:hasProvider   [ a  sripas:node_CTP ] 
          ] .</align:entity2>
				<align:relation>=</align:relation>
			</align:Cell>
		</align:map>
		<!--TODO check other instant type transformations -->
		<align:map>
			<align:Cell rdf:about="http://www.inter-iot.eu/sripas#timeContextEvent">
				<align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a                    context:ContextEvent, sripas:node_CTX ;
            sosa:phenomenonTime  [ a                    time:Instant ;
                                   time:inTimePosition  [ a                     time:TimePosition ;
                                                          time:numericPosition  [ a  sripas:node_time ]
                                                        ]
            
                                 ]
          ] .</align:entity1>
				<align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a                     context:ContextEvent, sripas:node_CTX ;
            context:hasTimestamp [ a  sripas:node_time ]
          ] .</align:entity2>
				<align:relation>=</align:relation>
				<sripas:typings rdf:parseType="Literal">
					<typing about="http://www.inter-iot.eu/sripas#node_time" datatype="http://www.w3.org/2001/XMLSchema#decimal" />
				</sripas:typings>
			</align:Cell>
		</align:map>
		<align:map>
			<align:Cell rdf:about="http://www.inter-iot.eu/sripas#timeContextEventFromResultTime">
				<align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a                    context:ContextEvent, sripas:node_CTX ;
            sosa:resultTime  [ a   sripas:xds-time] .
				</align:entity1>
				<align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a                     context:ContextEvent, sripas:node_CTX ;
            context:hasTimestamp [ a  sripas:node_time ]
          ] .</align:entity2>
				<align:relation>=</align:relation>
				<sripas:typings rdf:parseType="Literal">
					<typing about="http://www.inter-iot.eu/sripas#node_time" datatype="http://www.w3.org/2001/XMLSchema#decimal" />
				</sripas:typings>
			</align:Cell>
			<!-- TODO apply YEAR, MONTH, DAY, HOURS, MINUTES and SECONDS functions on sripas:xds-time to transform to time since epoch (sripas:node_time) -->
			<!-- https://en.wikipedia.org/wiki/Unix_time -->
		</align:map>
		<align:map>
			<align:Cell rdf:about="http://www.inter-iot.eu/sripas#bloodPreassureMeasurement">
				<align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a  context:ContextEvent,sripas:node_CTO  ;
            rdf:object  [ a              sosa:Result , &lt;http://inter-iot.eu/medex#SystolicBloodPressure&gt; , sripas:node_CTZ ;
                              iiot:hasValue  [ a  sripas:node_sys ]
                            ] ,
			    [ a              sosa:Result , &lt;http://inter-iot.eu/medex#DiastolicBloodPressure&gt; , sripas:node_CTY ;
                              iiot:hasValue  [ a  sripas:node_dias ]
                            ]
          ] .</align:entity1>
				<align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a  sripas:node_CTO ;
            rdf:object  [ a       healthMeasurement:BloodPressure , sripas:node_CTX ;
                              healthMeasurement:diastolicBloodPreassure
                                      [ a                  sripas:node_CTY ;
                                        measurement:value  [ a  sripas:node_dias ]
                                      ] ;
                              healthMeasurement:systolicBloodPreassure
                                      [ a                  sripas:node_CTZ ;
                                        measurement:value  [ a  sripas:node_sys ]
                                      ]
                            ]
          ] .</align:entity2>
				<align:relation>=</align:relation>
			</align:Cell>
		</align:map>
		<align:map>
			<align:Cell rdf:about="http://www.inter-iot.eu/sripas#changeUser">
				<!-- Change User Class; REVIEW after DM is updated with different User types -->
				<align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[  a  iiot:User , &lt;http://ontology.universAAL.org/Profile.owl#User&gt; , sripas:node_CTY] .</align:entity1>
				<align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string" />
				[ a  &lt;http://ontology.universAAL.org/Profile.owl#User&gt; , sripas:node_CTY ] .
				<align:relation>=</align:relation>
			</align:Cell>
		</align:map>
		<align:map>
			<align:Cell rdf:about="http://www.inter-iot.eu/sripas#changeUserProp">
				<!-- Change User Class -->
				<align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[ a        sripas:node_CTX ;
            iiot:orderedByUser  [ a  sripas:node_CTY ]
          ] .</align:entity1>
				<align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string" />
				[ a       sripas:node_CTX ;
            uaal:theInvolvedHumanUser  [ a  sripas:node_CTY ]
          ] .
				<align:relation>=</align:relation>
			</align:Cell>
		</align:map>
	</align:Alignment>
</rdf:RDF>
